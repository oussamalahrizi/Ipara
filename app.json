{
  "expo": {
    "name": "Ipara",
    "slug": "MyPara",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icons/ipara_logo.png",

    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/ipara_logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.ipara.app",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
